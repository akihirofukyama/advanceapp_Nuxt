{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ],
      "~~/*": [
        "./*"
      ],
      "@@/*": [
        "./*"
      ]
    }
      "types": [
        "@nuxt/types",
        "@nuxtjs/date-fns"
      ]
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ]
}